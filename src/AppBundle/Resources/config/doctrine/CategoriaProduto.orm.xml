<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CategoriaProduto" table="categoria_produto">
    <id name="idCategoriaPlanejamento" type="integer" column="id_categoria_planejamento">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomeCategoria" type="string" column="nome_categoria" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
